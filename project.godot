; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Path Of Legends"
run/main_scene="res://Levels/main.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"
Global="*res://Utils/Global.gd"
Transition="*res://Utils/Transition.tscn"
PlayerVariables="*res://Utils/PlayerVariables.gd"
PlayerUI="*res://UI/ui_canvas_layer.gd"
PlayerUICanvas="*res://UI/ui_canvas_layer.tscn"

[display]

window/size/viewport_width=990
window/size/viewport_height=540
window/stretch/mode="canvas_items"
window/stretch/scale=4.0

[editor_plugins]

enabled=PackedStringArray("res://addons/SignalVisualizer/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "red",
"res://Levels/": "purple",
"res://TileSets/": "green",
"res://UI/": "yellow",
"res://Utils/": "blue"
}

